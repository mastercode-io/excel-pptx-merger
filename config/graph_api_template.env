# Microsoft Graph API Configuration Template
# Copy this file to graph_api.env and fill in your Azure app credentials

# Azure App Registration Details
GRAPH_CLIENT_ID=your_client_id_here
GRAPH_CLIENT_SECRET=your_client_secret_here
GRAPH_TENANT_ID=your_tenant_id_here

# Graph API Settings
GRAPH_API_TIMEOUT=60
GRAPH_API_RETRY_ATTEMPTS=3
GRAPH_API_RETRY_DELAY=2

# Range Image Export Settings
RANGE_EXPORT_MAX_CELLS=10000
RANGE_EXPORT_DEFAULT_DPI=150
RANGE_EXPORT_DEFAULT_FORMAT=png
RANGE_EXPORT_TEMP_CLEANUP=true