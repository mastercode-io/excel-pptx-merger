{
  "version": "1.0",
  "sheet_configs": {
    "Order Form": {
      "subtables": [
        {
          "name": "report_info",
          "type": "key_value_pairs",
          "header_search": {
            "method": "contains_text",
            "text": "Search/Audit",
            "column": "E",
            "search_range": "E10:E20"
          },
          "data_update": {
            "orientation": "horizontal",
            "headers_row_offset": 0,
            "data_row_offset": 1,
            "max_columns": 1,
            "column_mappings": {
              "Search/Audit or Monitoring & Representation": {
                "name": "report_title",
                "type": "text"
              }
            }
          }
        },
        {
          "name": "client_info",
          "type": "key_value_pairs",
          "header_search": {
            "method": "contains_text",
            "text": "Client",
            "column": "A",
            "search_range": "A1:A10"
          },
          "data_update": {
            "orientation": "horizontal",
            "headers_row_offset": 0,
            "data_row_offset": 1,
            "max_columns": 6,
            "column_mappings": {
              "Client": {
                "name": "client_name",
                "type": "text"
              },
              "Word Or Image": {
                "name": "search_type",
                "type": "text"
              },
              "G&S Classes": {
                "name": "gs_classes",
                "type": "text"
              },
              "SIC": {
                "name": "sic_code",
                "type": "text"
              },
              "Nature of business": {
                "name": "business_nature",
                "type": "text"
              },
              "Designated Countries": {
                "name": "countries",
                "type": "text"
              }
            }
          }
        },
        {
          "name": "search_criteria",
          "type": "table",
          "header_search": {
            "method": "contains_text",
            "text": "Word",
            "column": "A",
            "search_range": "A3:A10"
          },
          "data_update": {
            "orientation": "vertical",
            "headers_row_offset": 0,
            "data_row_offset": 1,
            "max_columns": 3,
            "max_rows": 10,
            "column_mappings": {
              "Word": {
                "name": "word",
                "type": "text"
              },
              "Search Criteria": {
                "name": "search_criteria",
                "type": "text"
              },
              "Remarks": {
                "name": "remarks",
                "type": "text"
              }
            }
          }
        },
        {
          "name": "image_search",
          "type": "table",
          "header_search": {
            "method": "contains_text",
            "text": "Image",
            "column": "A",
            "search_range": "A10:A20"
          },
          "data_update": {
            "orientation": "vertical",
            "headers_row_offset": 0,
            "data_row_offset": 1,
            "max_columns": 3,
            "max_rows": 10,
            "column_mappings": {
              "Image": {
                "name": "image",
                "type": "image"
              },
              "Search Criteria": {
                "name": "search_criteria",
                "type": "text"
              },
              "Image Class.Division.Subdivision": {
                "name": "image_classification",
                "type": "text"
              }
            }
          }
        }
      ]
    }
  },
  "global_settings": {
    "normalize_keys": true,
    "temp_file_cleanup": {
      "enabled": true,
      "delay_seconds": 300,
      "keep_on_error": true,
      "development_mode": false
    },
    "image_compression": {
      "enabled": false,
      "threshold_mb": 2,
      "quality": 80
    }
  }
}